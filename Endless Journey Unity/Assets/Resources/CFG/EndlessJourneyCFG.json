{
  "num_angles": [ "#discrete 1~8#" ],
  "num_plants": [ "#discrete 80~125#" ],
  "num_animals": [ "#discrete 20~35#" ],
  "num_clouds": [ "#discrete 60~110#" ],
  "num_rocks": [ "#discrete 100~200#" ],
  "scale_plant": [ "\"scale\";;;#random 0.3~8.5#" ],
  "scale_rock": [ "\"scale\";;;#random 0.2~14#" ],
  "scale_animal": [ "\"scale\";;;#random 0.6~3.7#" ],
  "scale_cloud": [ "\"scale\";;;#random 1~20#" ],
  "animal_angle": ["\"animal_angle\";;;#random 0~1#"],
  "animal_height": ["\"animal_height\";;;#random 17~30#"],
  "cloud_height": ["\"cloud_height\";;;#random 70~200#"],
  "music": [ "\"music\";;;@!!{\"t1\";;;#discrete 0~2#, \"v1\";;;#random 0.5~1#}, {\"t2\";;;#discrete 0~2#, \"v2\";;;#random 0.5~1#}, {\"t3\";;;#discrete 0~2#, \"v3\";;;#random 0.5~1#}, !!@" ],
  "section": [ "\"angles\";;;#angles#, #path#" ],
  "path": [ "\"path_glow\";;;#glow#, \"path_threshold\";;;#random 0.8~1.0#" ],
  "background": [ "#ground#, #items#" ],
  "ground": [ "#metallic#, #smoothness#" ],
  "items": [ "\"rocks\";;;@!!<#num_rocks# {#item_pos#, #rock_subtype#, #scale_rock#},>!!@, \"plants\";;;@!!<#num_plants# {#item_pos#, #plant_subtype#, #scale_plant#},>!!@, \"animals\";;;@!!<#num_animals# {#item_pos#, #animal_subtype#, #scale_animal#, #animal_angle#, #animal_height#},>!!@, \"clouds\";;;@!!<#num_clouds# {#item_pos#, #cloud_subtype#, #scale_cloud#, #cloud_height#},>!!@" ],
  "item_pos": [ "\"item_x\";;;#random -1~1#, \"item_z\";;;#random -1~1#" ],
  "rock_subtype": ["\"rock_subtype\";;;#discrete 1~1#"],
  "plant_subtype": [ "\"plant_subtype\";;;#discrete 1~3#" ],
  "animal_subtype": [ "\"animal_subtype\";;;#discrete 1~2#" ],
  "cloud_subtype": [ "\"cloud_subtype\";;;#discrete 1~2#" ],
  "sky_gradient": [ "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"no_gradient\"", "\"sky_gradient\";;;\"gradient\"", "\"sky_gradient\";;;\"gradient\"", "\"sky_gradient\";;;\"gradient\"" ],
  "color_scheme": [ "\"color_scheme\";;;#discrete 0~1713#" ],
  "metallic": [ "\"metallic\";;;#random 0~0.8#" ],
  "smoothness": [ "\"smoothness\";;;#random 0~0.7#" ],
  "glow": [ "#random 0.4~1.1#" ],
  "weather_active": [ "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;false", "\"weather_active\";;;true", "\"weather_active\";;;true" ],
  "wind_zone": ["\"wind_active\";;;#discrete 0~1#, \"wind_main\";;;#random 0.4~4#, \"wind_turbulence\";;;#random 0.1~2.2#, \"wind_pulseMag\";;;#random 5~20#, \"wind_pulseFreq\";;;#random 0.2~1.8#"],
  "weather": ["\"weather_type\";;;#discrete 0~0#, \"weather_gravity\";;;#random 1.5~7#, #weather_active#, #wind_zone#"],
  "section_length": [ "\"section_length\";;;#discrete 200~400#" ],
  "angles": [ "@!!<#num_angles# {\"pos_x\";;;#random 0~1#, \"pos_z\";;;#random 0~1#, \"angle\";;;#random 0~1#},>!!@" ],
  "start": [ "{#section_length#, #color_scheme#, #sky_gradient#, #section#, #background#, #music#, #weather#}" ]
}